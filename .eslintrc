{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "new-cap": "off",
    "no-unused-vars": "off",
    "indent": [
      "error",
      2,
      {
        "CallExpression": {
          "arguments": "off"
        }
      }
    ],
    "comma-dangle": "off",
    "max-len": "off",
    "prettier/prettier": "off"
  }
}
